actions
(
    // For easier post-processing, and data-conversion tests
    {
        name    inner;
        type    cellZoneSet;
        action  new;
        source  boxToCell;
        min     (-2.5 -0.8 -0.8);
        max     (0.8 0.8 0.8);
    }
);


